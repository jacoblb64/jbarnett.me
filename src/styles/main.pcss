@import "xp.css/dist/xp.css";

@tailwind base;

/* custom styles go here */
:root {
    --xp-red: #DE482B;
    --xp-yellow: #FCCF03;
    --xp-blue: #3B77BC;
    --xp-green: #81C046;
}

.office-colors {
    background: linear-gradient(
        to right,
        var(--xp-red) 25%,
        var(--xp-yellow) 50%,
        var(--xp-green) 75%,
        var(--xp-blue) 100%
    )
}

@tailwind components;

@tailwind utilities;
